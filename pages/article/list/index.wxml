<view class="weui-panel weui-panel_access" style="{{ systemInfo.safeAreaInset && 'padding-bottom: 34px !important' }}">
  <view class="weui-panel__bd">
    <navigator url="" class="weui-media-box weui-media-box_appmsg" hover-class="weui-cell_active" wx:for="{{ data }}">
      <view class="weui-media-box__hd weui-media-box__hd_in-appmsg">
        <image class="weui-media-box__thumb" src="{{ item.thumbnail }}" lazy-load mode="aspectFill"/>
      </view>
      <view class="weui-media-box__bd weui-media-box__bd_in-appmsg">
        <view class="weui-media-box__title">{{ item.title }}</view>
        <view class="weui-media-box__desc">{{ item.subtitle }} {{ item.description }}</view>
      </view>
    </navigator>
  </view>
</view>